@model IEnumerable<Application.DTO.BookSeriesDTO>
<!DOCTYPE html>
<html>
<head>
    <title>Серии</title>
</head>
<body>
    <h3>Серии</h3>
    <form method="get" action="~/addBookSeries">
        <table>
            <tr>
                <td>Id</td>
                <td>Серия</td>
                <td></td>
            </tr>
            @foreach (var series in Model)
            {
                <tr>
                    <td>@series.Id</td>
                    <td>@series.SeriesName</td>
                    <td><a href="~/Change/@series.Id">Изменить</a></td>
                </tr>
            }
        </table>
        <br />
        <button>Добавить серию</button>
    </form>
</body>
</html>